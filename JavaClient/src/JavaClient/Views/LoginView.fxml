<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="164.0" prefWidth="315.0"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="JavaClient.Controllers.LoginController">
    <children>
        <Label layoutX="47.0" layoutY="38.0" text="Username"/>
        <Label layoutX="49.0" layoutY="67.0" text="Password"/>
        <TextField fx:id="UsernameField" layoutX="114.0" layoutY="34.0"/>
        <PasswordField fx:id="PasswordField" layoutX="114.0" layoutY="63.0"/>
        <Button layoutX="105.0" layoutY="112.0" mnemonicParsing="false" onAction="#OnLoginButtonPressed" text="Login"/>
        <Button layoutX="165.0" layoutY="112.0" mnemonicParsing="false" onAction="#OnRegisterButton" text="Register"/>
    </children>
</AnchorPane>
